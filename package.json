{
  "name": "elrepecho",
  "browserslist": "> 0.5%, last 4 versions, not dead",
  "version": "1.0.0",
  "description": "Web de un grupo ciclista donde se muestran imágenes, calendario de salidas, etc.",
  "scripts": {
    "parcel:desarrollo": "parcel fuente/index.html --dist-dir 'desarrollo'",
    "parcel:produccion": "parcel build fuente/index.html --public-url './' --dist-dir 'produccion'",
    "limpia": "rimraf desarrollo produccion .parcel-cache",
    "des": "npm run parcel:desarrollo",
    "prod": "npm-run-all limpia parcel:produccion"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avianarios/webciclismo.git"
  },
  "author": "Alejandro Viana Ríos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avianarios/webciclismo/issues"
  },
  "homepage": "https://github.com/avianarios/webciclismo#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "parcel": "^2.8.3",
    "parcel-plugin-imagemin": "^4.0.2",
    "sass": "^1.57.1",
    "rimraf": "^4.1.2",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "remixicon": "^2.5.0"
  }
}
